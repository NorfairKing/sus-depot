card xmonad {
    alternatives $(HOSTNAME) $(HOST) shared
    into ~/.xmonad

    outof xmonad

    xmonad.hs
    makefile

    {
        into lib

        Actions.hs
        Constants.hs
        Internet.hs
        Keys.hs
        Layout.hs
        Workspaces.hs
        Modifier.hs
        Solarized.hs
    }
}
