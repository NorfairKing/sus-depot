add_tmppath "/usr/local/sbin"
add_tmppath "/usr/local/bin"
add_tmppath "/usr/sbin"
add_tmppath "/usr/bin"
add_tmppath "/sbin"
add_tmppath "/bin"

add_tmppath "/usr/local/lib"
