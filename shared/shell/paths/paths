# Bin
add "$HOME/scripts"
add "$HOME/.local/bin"
add "$HOME/.cabal/bin"
add "/usr/lib/python3.4/site-packages"

add "/usr/local/sbin"
add "/usr/local/bin"
add "/usr/sbin"
add "/usr/bin"
add "/sbin"
add "/bin"

# Lib
add "/usr/local/lib"
add "/usr/lib"
add "/lib"

# Go
export GOPATH="$HOME/.gopath"
add "$GOPATH"
add "$GOPATH/bin"

# Specific to system
safe_source ~/.paths/specific
