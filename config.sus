card config {
  alternatives $(HOST) shared
  alternatives $(USER) shared
  into ~/.config

  redshift.conf

  {
    into zathura
    zathurarc
  }

  {
    into smos
    outof smos
    config.yaml
  }

  {
    outof config

    {
      outof xdg
      user-dirs.locale
      user-dirs.dirs
    }
  }
}
